{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier", "sql"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-unused-vars": 0,
    "camelcase": 0,
    "consistent-return": 0,
    "sql/format": [
      2,
      {
        "ignoreExpressions": false,
        "ignoreInline": false,
        "ignoreTagless": false
      }
    ],
    "sql/no-unsafe-query": [
      2,
      {
        "allowLiteral": false
      }
    ]
  }
}
